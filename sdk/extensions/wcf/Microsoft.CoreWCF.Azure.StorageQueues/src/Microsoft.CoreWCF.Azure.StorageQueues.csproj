<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RequiredTargetFrameworks>netstandard2.0</RequiredTargetFrameworks>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <AssemblyName>Microsoft.CoreWCF.Azure.StorageQueues</AssemblyName>
    <PackageId>Microsoft.CoreWCF.Azure.StorageQueues</PackageId>
    <RootNamespace />
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <Version>1.0.0-beta.0</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="CoreWCF\Configuration\**" />
    <EmbeddedResource Remove="CoreWCF\Configuration\**" />
    <None Remove="CoreWCF\Configuration\**" />
  </ItemGroup>
  
  <ItemGroup>
    <PackageReference Include="Azure.Storage.Queues" />
		<PackageReference Include="CoreWCF.Queue" />
		<PackageReference Include="Microsoft.Extensions.Http" />
		<PackageReference Include="System.IO.Pipelines" />
	</ItemGroup>

</Project>

